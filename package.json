{
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "build": "vite build",
    "preview": "vite preview --port 80",
    "dev": "vite --port 80",
    "lint": "eslint \"src/**/*.{vue,ts,js}\"",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "taze": "npx taze major --exclude vue,vue-template-compiler,vue-router,vite && npx taze patch --include vue,vue-template-compiler,vue-router,vite",
    "taze:update": "npx taze major -w --exclude vue,vue-template-compiler,vue-router,vite && npx taze patch -w --include vue,vue-template-compiler,vue-router,vite && pnpm i"
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.0",
    "@vueuse/core": "^8.9.2",
    "vue": "2.6.14",
    "vue-router": "3.5.4"
  },
  "devDependencies": {
    "@byyuurin/eslint-config": "^0.6.1",
    "@iconify/json": "^2.1.77",
    "@unocss/reset": "^0.44.2",
    "@unocss/transformer-directives": "^0.44.2",
    "@vue/runtime-dom": "latest",
    "eslint": "^8.19.0",
    "pnpm": "^7.5.2",
    "typescript": "^4.7.4",
    "unocss": "^0.44.2",
    "unplugin-auto-import": "^0.9.3",
    "unplugin-vue-components": "^0.21.1",
    "unplugin-vue2-script-setup": "^0.11.2",
    "vite": "^2.9.14",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue2": "^2.0.1",
    "vitest": "^0.18.0",
    "vue-template-compiler": "2.6.14",
    "vue-tsc": "^0.38.5"
  }
}
