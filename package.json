{
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "build": "vite build",
    "preview": "vite preview --port 80",
    "dev": "vite --port 80",
    "lint": "eslint \"src/**/*.{vue,ts,js}\"",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "taze": "npx taze major --exclude vue,vue-router && npx taze patch --include vue,vue-router",
    "taze:update": "npx taze major -w --exclude vue,vue-router && npx taze patch -r --include vue,vue-router && pnpm i"
  },
  "dependencies": {
    "@vue/composition-api": "^1.6.1",
    "@vueuse/core": "^8.4.2",
    "vue": "2.6.14",
    "vue-router": "3.5.3"
  },
  "devDependencies": {
    "@byyuurin/eslint-config": "^0.5.1",
    "@iconify/json": "^2.1.40",
    "@unocss/reset": "^0.33.2",
    "@unocss/transformer-directives": "^0.33.2",
    "@vue/runtime-dom": "latest",
    "eslint": "^8.15.0",
    "pnpm": "^7.0.1",
    "typescript": "^4.6.4",
    "unocss": "^0.33.2",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "unplugin-vue2-script-setup": "^0.10.2",
    "vite": "^2.9.8",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-vue2": "^2.0.0",
    "vitest": "^0.12.4",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.12"
  }
}
