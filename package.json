{
  "private": true,
  "packageManager": "pnpm@6.32.3",
  "scripts": {
    "build": "vite build",
    "preview": "vite preview --port 80",
    "dev": "vite --port 80",
    "lint": "eslint \"src/**/*.{vue,ts,js}\"",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "taze": "npx taze major --exclude vue,vue-router && npx taze patch --include vue,vue-router",
    "taze:update": "npx taze major -w --exclude vue,vue-router && npx taze patch -r --include vue,vue-router && pnpm i"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.9",
    "@vueuse/core": "^8.2.5",
    "vue": "2.6.14",
    "vue-router": "3.5.3"
  },
  "devDependencies": {
    "@byyuurin/eslint-config": "^0.5.1",
    "@iconify/json": "^2.1.28",
    "@unocss/reset": "^0.31.2",
    "@unocss/transformer-directives": "^0.31.2",
    "@vue/runtime-core": "latest",
    "@vue/runtime-dom": "latest",
    "eslint": "^8.13.0",
    "pnpm": "^6.32.6",
    "typescript": "^4.6.3",
    "unocss": "^0.31.2",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.2",
    "unplugin-vue2-script-setup": "^0.10.2",
    "vite": "^2.9.1",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-vue2": "^1.9.3",
    "vitest": "^0.9.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.34.6"
  }
}
